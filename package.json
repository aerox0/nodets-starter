{
	"name": "nodets-starter",
	"version": "1.0.0",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"build": "swc src -d dist",
		"start": "node dist/index.js",
		"dev": "nodemon -L -e ts --exec \"npm run debug\"",
		"debug": "ts-node --swc src/index.ts",
		"bump": "standard-version --skip.tag",
		"release": "git flow release start",
		"pubpush": "git flow release finish"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aerox0/nodets-starter.git"
	},
	"author": "aerox0",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/aerox0/nodets-starter/issues"
	},
	"homepage": "https://github.com/aerox0/nodets-starter#readme",
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.241",
		"@types/node": "^18.7.8",
		"commitizen": "^4.2.5",
		"cz-conventional-changelog": "3.3.0",
		"nodemon": "^2.0.19",
		"prettier": "^2.7.1",
		"standard-version": "^9.5.0",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
